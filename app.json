{
  "expo": {
    "name": "mybuddy-app",
    "slug": "mybuddy-app",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/images/splash.png",
          "resizeMode": "cover",
          "imageWidth": 300,
          "dark": {
            "image": "./assets/images/splash.png",
            "resizeMode": "cover",
            "backgroundColor": "#ffffff"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "enableBackgroundRemoteNotifications": false
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "orientation": "portrait",
    "icon": "./assets/images/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFFFFF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "L'application nécessite un accès à la bibliothèque de photos pour enregistrer des vidéos et des images.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyA75KUNfa4gceqWQ7M-yBjIRqbXRdA4vOI"
      },
      "supportsTablet": true,
      "bundleIdentifier": "com.csmgaussin.mybuddydiary",
      "buildNumber": "117"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash.png",
        "backgroundColor": "#f8f8f8"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyA75KUNfa4gceqWQ7M-yBjIRqbXRdA4vOI"
        }
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.MODIFY_AUDIO_SETTINGS",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ],
      "package": "com.csmgaussin.mybuddydiary"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/9f79f752-81c4-4a4d-b8bd-ee4da5b13c32"
    },
    "owner": "cgaussin",
    "extra": {
      "eas": {
        "projectId": "9f79f752-81c4-4a4d-b8bd-ee4da5b13c32"
      }
    }
  }
}
